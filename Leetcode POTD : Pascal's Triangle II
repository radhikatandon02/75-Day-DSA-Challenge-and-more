class Solution {
public:
    vector<int> getRow(int rowIndex) {
        vector<int> result(1,1);
        long long prev = 1;
        for(int k=1; k<= rowIndex; k++)
        {
            long long next = prev * (rowIndex - k + 1)/k;
            result.push_back(next);
            prev = next;
        }
        return result;
    }
};
